{
  "name": "bolivar-paralelo",
  "description": "An open source to way to get bolivar's exchange rate",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "cp -R node_modules/flag-icon-css/flags/4x3 static/flags; cp -R node_modules/@fortawesome/fontawesome-free/svgs/solid static/fa-icons; sapper build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.0",
    "chart.js": "^2.9.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.18.2",
    "express-promise-router": "^4.0.1",
    "flag-icon-css": "^3.5.0",
    "js-cookie": "^2.2.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.28.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.9",
    "svelte": "^3.29.0",
    "svelte-check": "^1.0.54",
    "svelte-preprocess": "^4.3.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
